@article{Fodor2002,
	Abstract = {Based on universal arguments it is believed that there is a critical point (E) in QCD on the temperature ( T ) versus chemical potential ({\^I}{\OE}) plane, which is of extreme importance for heavy-ion experiments. Using finite size scaling and a recently proposed lattice method to study QCD at finite {\^I}{\OE} we determine the location of E in QCD with n f = 2+1 dynamical staggered quarks with semi-realistic masses on L t = 4 lattices. Our result is T E = 160{\^A}$\pm$3.5 MeV and {\^I}{\OE} E = 725{\^A}$\pm$35 MeV. For the critical temperature at {\^I}{\OE} = 0 we obtained T c = 172{\^A}$\pm$3 MeV.},
	Author = {Zoltan Fodor and Sandor D. Katz},
